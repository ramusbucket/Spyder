@using EMS.Web.Server.Collector.Models
<div class="help-block"></div>
<div class="jumbotron text-center">
    <h1 class="text-center">Data collector service</h1>
    <h2>Status:<span class="text-success">Active</span></h2>
</div>

<div class="row">
    @foreach (var counter in CollectorStatistics.Counters)
    {
        <div class="col-lg-12">
            <h3>
                @counter.Key
                <span class="text-success">@counter.Value</span>
            </h3> 
        </div>
    }
</div>